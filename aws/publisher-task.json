{
    "family": "publisher",
    "containerDefinitions": [
        {
            "name": "benchmarker",
            "image": "924899176789.dkr.ecr.ap-northeast-1.amazonaws.com/benchmarker:latest",
            "cpu": 0,
            "portMappings": [],
            "essential": true,
            "environment": [
                {
                    "name": "MODE",
                    "value": "PUB"
                },
                {
                    "name": "BROKER_URL",
                    "value": "tcp://broker.iot-simulator"
                },
                {
                    "name": "NUM_CLIENTS",
                    "value": "1"
                },
                {
                    "name": "QOS",
                    "value": "0"
                },
                {
                    "name": "EXECUTION_TIME",
                    "value": "60"
                },
                {
                    "name": "MESSAGE_SIZE",
                    "value": "100"
                }, 
                {
                    "name": "THROUGHPUT",
                    "value": "0"
                }, 
               {
                    "name": "UNIT",
                    "value": "msg/s"
                },

                {
                    "name": "TOPIC",
                    "value": "test"
                },
                {
                    "name": "START_TOPIC",
                    "value": "start"
                }
            ],
            "environmentFiles": [],
            "volumesFrom": [],
            "ulimits": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::924899176789:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "4096",
    "memory": "8192",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
